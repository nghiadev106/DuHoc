@model DuHoc.ViewModels.FeedBack.FeedBackCreateRequest
<div class="contact_page">
    <div class="container ">
        <div class="col_contact">
            <h1 class="page-title hidden">Liên hệ</h1>
            <div class="entry_content">

                <div class="row">
                    <div class="col-md-6">
                        <h3 class="ft_title">Trụ sở chính</h3>
                        <h3 class="ft_title_ct"><span class="fa fa-th"></span> CÔNG TY CỔ PHẦN TƯ VẤN DU HỌC VÀ THƯƠNG MẠI VJ</h3>
                        <div class="ft_infos">
                            <ul class="ft_list">
                                <li>Địa chỉ : Nhà T4-BT1-L4, Đường Trung Văn Phường Trung Văn, Quận Nam Từ Liêm, Thành phố Hà Nội</li>
                                <li>Điện thoại : <a href="tel:092.405.2222">092.405.2222</a></li>
                                <li>Hotline : <a href="tel:092.405.2222">092.405.2222</a></li>
                                <li>Facebook : <a href="https://www.facebook.com/Cong.Ty.VJ/">https://www.facebook.com/Cong.Ty.VJ/</a></li>
                                <li>Youtube : <a href="https://www.youtube.com/channel/UCj4j_q3VESYORPwvXVSnVeA">https://www.youtube.com/channel/UCj4j_q3VESYORPwvXVSnVeA</a></li>
                            </ul>
                        </div>
                        <div class="box_map">
                            <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3725.0745264714296!2d105.78268531533173!3d20.989649494487313!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x3135acb7941323f7%3A0xd1c51b452b7c081e!2zVHJ1bmcgVsSDbiwgTmFtIFThu6sgTGnDqm0sIEjDoCBO4buZaSwgVmnhu4d0IE5hbQ!5e0!3m2!1svi!2s!4v1633775158018!5m2!1svi!2s" width="600" height="450" style="border:0;" allowfullscreen="" loading="lazy"></iframe>
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div class="contact_form qcol form_info_customer">
                            <h3 class="ft_title">Liên hệ với chúng tôi</h3>
                            <div role="form">
                                <div class="screen-reader-response"><p role="status" aria-live="polite" aria-atomic="true"></p> <ul></ul></div>
                                <form role="form" asp-action="Register" asp-controller="Home" method="post" class="wpcf7-form init" novalidate="novalidate">
                                    <p>
                                        <label>Họ tên</label><br />
                                        <span class="wpcf7-form-control-wrap your-name">
                                            <input type="text" asp-for="Name" value="" size="40" class="wpcf7-form-control wpcf7-text form-control" aria-invalid="false" placeholder="Họ &amp; Tên" />
                                        </span>
                                        <span asp-validation-for="Name" class="text-danger"></span>
                                    </p>
                                    <div class="row">
                                        <div class="col-md-6" style="margin-bottom:20px;">
                                            <label>Email</label><br />
                                            <span class="wpcf7-form-control-wrap your-email">
                                                <input type="email" asp-for="Email" value="" size="40" class="wpcf7-form-control wpcf7-text wpcf7-email wpcf7-validates-as-required wpcf7-validates-as-email form-control" aria-required="true" aria-invalid="false" placeholder="Email" />
                                                <span asp-validation-for="Email" class="text-danger"></span>
                                            </span>
                                        </div>
                                        <div class="col-md-6" style="margin-bottom:20px;">
                                            <label>SĐT</label><br />
                                            <span class="wpcf7-form-control-wrap your-tel"><input type="tel" asp-for="Phone" value="" size="40" class="wpcf7-form-control wpcf7-text wpcf7-tel wpcf7-validates-as-required wpcf7-validates-as-tel form-control" aria-required="true" aria-invalid="false" placeholder="SĐT" /></span>
                                            <span asp-validation-for="Phone" class="text-danger"></span>
                                        </div>
                                    </div>
                                    <p>
                                        <label>Địa chỉ</label><br />
                                        <span class="wpcf7-form-control-wrap your-address"><input type="text" asp-for="Address" value="" size="40" class="wpcf7-form-control wpcf7-text form-control" aria-invalid="false" placeholder="Địa chỉ" /></span>
                                        <span asp-validation-for="Address" class="text-danger"></span>
                                    </p>
                                    <p>
                                        <label>Lời nhắn</label><br />
                                        <span class="wpcf7-form-control-wrap your-message">
                                            <textarea asp-for="Message" cols="40" rows="10" class="wpcf7-form-control wpcf7-textarea form-control" aria-invalid="false" placeholder="Lời nhắn"></textarea>
                                        </span>
                                        <span asp-validation-for="Message" class="text-danger"></span>
                                    </p>
                                    <p class="submit_ct"><span class="fa fa-envelope"></span><input type="submit" value="Gửi thông tin" class="wpcf7-form-control has-spinner wpcf7-submit" /></p>
                                    <div class="wpcf7-response-output" aria-hidden="true"></div>
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>